{
    "name": "ReloadTimer",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "ReloadTimer",
    "permissions": ["activeTab","declarativeContent", "storage", "tabs"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
          "144": "images/icon.png"
        }
    },
    "background": {
        "scripts": ["background.js"]
    }
  }